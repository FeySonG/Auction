<!DOCTYPE HTML>

<html>
<head>
	@await Html.PartialAsync("MetaTagsPartial")
	@await Html.PartialAsync("CssPartial")

</head>
<body class="homepage is-preload">
	<div id="page-wrapper">

		@await Html.PartialAsync("HeaderPartial")

		<!-- Main -->
		<section id="main">
			<div class="container">
				<div class="row">
					<div class="col-12">
						@RenderBody()
					</div>
					@await Html.PartialAsync("SidebarPartial")

				</div>
			</div>
		</section>

		@await Html.PartialAsync("FooterPartial")
	</div>

	<!-- Scripts -->
	@await Html.PartialAsync("ScriptPartial")
	@await RenderSectionAsync("Scripts", required: false)

</body>
</html>